{
    "compilerOptions": {
        "jsx": "react",
        "target": "ES5",
        "module": "CommonJS",
        "lib": ["DOM", "ES2015"],
        "esModuleInterop": true,
        "allowJs": true,
        "strict": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "declaration": true,
        "outDir": "dist"
    },
    "exclude": ["node_modules", "dist"]
}
